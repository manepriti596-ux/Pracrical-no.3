#include <iostream>
using namespace std;

int main()
{
    int n;
    float key;

    cout << "Enter number of elements: ";
    cin >> n;

    float arr[n];

    cout << "Enter sorted float elements:\n";
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    cout << "Enter value to search: ";
    cin >> key;

    int low = 0, high = n - 1, mid;

    while (low <= high)
    {
        mid = (low + high) / 2;

        if (arr[mid] == key)
        {
            cout << "Value found at position: " << mid + 1;
            return 0;
        }
        else if (arr[mid] < key)
        {
            low = mid + 1;
        }
        else
        {
            high = mid - 1;
        }
    }

    cout << "Value not found";
    return 0;
}
