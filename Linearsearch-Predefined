#include <iostream>
using namespace std;

int main()
{
    int arr[5] = {5, 10, 15, 20, 25};
    int key, i;

    cout << "Enter element to search: ";
    cin >> key;

    for (i = 0; i < 5; i++)
    {
        if (arr[i] == key)
        {
            cout << "Element found at position: " << i + 1;
            break;
        }
    }

    if (i == 5)
    {
        cout << "Element not found";
    }

    return 0;
}
